<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>David Tabatadze</Authors>
    <Description>Audit logging functionality for .net core web application</Description>
    <Copyright>Copyright (c) 2020 David Tabatadze</Copyright>
    <PackageLicenseUrl>https://github.com/davidtabatadze/AuditLog/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/davidtabatadze/AuditLog</PackageProjectUrl>
    <RepositoryUrl>https://github.com/davidtabatadze/AuditLog</RepositoryUrl>
    <RepositoryType>C#</RepositoryType>
    <PackageTags>.net core elastic audit logger david tabatadze</PackageTags>
    <PackageReleaseNotes>AuditLogger</PackageReleaseNotes>
    <NeutralLanguage>ka-GE</NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CoreKit.Extension" Version="2.0.6" />
    <PackageReference Include="CoreKit.Sync" Version="1.0.1" />
    <PackageReference Include="Microsoft.AspNetCore.Http" Version="2.2.2" />
    <PackageReference Include="NEST" Version="7.10.0" />
  </ItemGroup>

</Project>
